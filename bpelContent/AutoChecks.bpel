<!-- AutoChecks BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Wed May 13 15:26:41 AEST 2015 -->
<bpel:process name="AutoChecks"
         targetNamespace="http://soacourse.unsw.edu.au/autochecker"
         suppressJoinFailure="yes"
         xmlns:tns="http://soacourse.unsw.edu.au/autochecker"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://topdown.soacourse.unsw.edu.au"
         xmlns:ns2="http://psp.soacourse.unsw.edu.au">
       
    <!-- Import the client WSDL -->
	<bpel:import location="AutoChecksArtifacts.wsdl" namespace="http://soacourse.unsw.edu.au/autochecker" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
	<bpel:import location="greenSlipService.wsdl" namespace="http://topdown.soacourse.unsw.edu.au"
	        importType="http://schemas.xmlsoap.org/wsdl/" />
       
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:AutoChecks"
                     myRole="gschecker"
                     />
        <bpel:partnerLink name="gschecker"
                     partnerLinkType="tns:AutoChecks"
                     partnerRole="gschecker"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="check"
                  messageType="ns1:GSCheck"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="checkInfo"
                  messageType="ns1:GSCheckResponse"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in AutoChecks.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="ns1:GSSTopdown"
                 operation="GSCheck" variable="check"
                 createInstance="yes"/>
        <bpel:invoke name="Invoke" partnerLink="gschecker"
               portType="ns1:GSSTopdown"
               operation="GSCheck" inputVariable="check"
               outputVariable="checkInfo">
        </bpel:invoke>
        <!-- Generate reply to synchronous request -->
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="ns1:GSSTopdown"
               operation="GSCheck" 
               variable="checkInfo"
               />
    </bpel:sequence>
</bpel:process>

